{
    "version": 2,
    "builds": [
      {
        "src": "server.js",
        "use": "@vercel/node"
      }
    ],
    "routes": [
      {
        "src": "/api/lineups",
        "dest": "server.js"
      },
      {
        "src": "/api/players",
        "dest": "server.js"
      },
      {
        "src": "/(.*)",
        "dest": "/"
      }
    ]
  }
  