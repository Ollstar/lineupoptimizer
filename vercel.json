{
  "version": 2,
  "name": "lineup-optimizer",
  "builds": [
    {
      "src": "server.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/server.js"
    },
    {
      "src": "/",
      "dest": "/index.html"
    },
    {
      "src": "/(.*).(js|css)",
      "dest": "/$1.$2"
    }
  ],
  "framework": "node"

}
